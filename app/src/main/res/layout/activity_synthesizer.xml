    <Button
        android:id="@+id/button_synth_highgs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="highgs"
        app:layout_constraintStart_toEndOf="@+id/button_synth_highfs"
        app:layout_constraintTop_toBottomOf="@+id/button_synth_highe" />

    <Button
        android:id="@+id/button_synth_highg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="highg"
        app:layout_constraintStart_toEndOf="@+id/button_synth_highgs"
